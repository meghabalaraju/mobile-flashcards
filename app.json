{
  "expo": {
    "name": "mobile-flashcards",
    "slug": "mobile-flashcards",
    "description": "The polar question flashcards app",
    "sdkVersion": "39.0.0",
    "platforms": [
      "android"
    ],
    "githubUrl": "https://github.com/meghabalaraju/mobile-flashcards",
    "notification": {
      "icon": "./assets/flashcards.png"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#292929"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "technovertex.mobileflashcards.app",
      "useNextNotificationsApi": true,
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "superuser_1"
  }
}
